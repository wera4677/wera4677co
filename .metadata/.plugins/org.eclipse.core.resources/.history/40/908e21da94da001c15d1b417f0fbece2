package dayday01;

import java.util.Scanner;



public class day16 {

	public static void main(String[] args) {
		
		int num1 = 0 ;
		int num2 = 0;
		int num3 = 0;
		Scanner sc = new Scanner(System.in);
	
		num1 = sc.nextInt();
		num2 = sc.nextInt();
		num3 = sc.nextInt();
		
		System.out.print("세수의 합   : ");
		System.out.println(num1+num2+num3);
	}

}
